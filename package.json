{
  "name": "titanium-navigator",
  "version": "0.1.0",
  "description": "Enables Angular and Vue.js routing in Titanium",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.prod.cjs.json",
    "build:esm": "tsc -p tsconfig.prod.esm.json",
    "clean": "rimraf dist",
    "test": "npm run lint",
    "lint": "tslint -p tsconfig.json",
    "coveralls": "karma start karma.unit.config.js | coveralls",
    "prepack": "npm run build"
  },
  "keywords": [
    "titanium",
    "navigator",
    "navigation",
    "router"
  ],
  "author": "Axway Appcelerator",
  "license": "Apache-2.0",
  "dependencies": {
    "strongly-typed-events": "^1.4.6",
    "titanium-vdom": "^0.3.0"
  },
  "devDependencies": {
    "@types/titanium": "^7.1.0",
    "rimraf": "^2.6.2",
    "tslint": "^5.10.0",
    "tslint-eslint-rules": "^5.3.1",
    "typescript": "^2.9.1"
  }
}
